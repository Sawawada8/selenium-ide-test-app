Table admins as A {
    id int [pk, increment]
    name varchar
    email varchar
    password varchar
}

Table users as U {
    id int [pk, increment]
    name varchar
    email varchar
    password varchar
    role tinyint
}

Ref: projects.user_id > users.id
Table projects {
    id int [pk, increment]
    user_id int [ref: > users.id]
    name varchar
}

// projects の子テーブル
Table sections {
    id int [pk, increment]
    project_id int [ref: > projects.id]
    script_url varchar
}


Table labels {
    id int [pk, increment]
    label varchar
}

// label projet 中間テーブル
Table label_project {
    id int [pk, increment]
    label_id int [ref: > labels.id]
    project_id int [ref: > project.id]
}

Table roles {
    id int [pk, increment]
    name varchar
}

Table user_role {
    id int [pk, increment]
    user_id int [ref: > users.id]
    role_id int [ref: > roles.id]
}

